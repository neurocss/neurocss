{
  "name": "@neurocss/monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.13.1",
  "scripts": {
   "postinstall": "esno scripts/prepare.ts",
   "freshdeps": "freshdeps minor -wIr && pnpm -r --parallel run update-post",
   "bench": "npm -C bench run bench",
   "build": "rimraf packages/*/dist --glob && esno scripts/copy-files.ts && pnpm -r --filter=./packages/* run build && pnpm -r run build-post",
   "dev": "nyxr stub",
   "deploy": "nyxr build && npm -C docs run docs:build && npm -C playground run build && npm -C interactive run build",
   "interactive": "npm -C interactive run dev",
   "docs": "npm -C docs run docs:dev",
   "inspector": "npm -C packages/inspector run dev",
   "play": "npm -C playground run dev",
   "lint": "eslint --cache .",
   "lint:fix": "nyxr lint --fix",
   "release": "bumping -r",
   "size": "esno scripts/size.ts",
   "stub": "pnpm -r --filter=./packages/* --parallel run stub",
   "typecheck": "tsc --noEmit && pnpm -C packages/eslint-plugin run typecheck",
   "test": "vitest",
   "test:update": "vitest -u",
   "test:integration": "pnpm -F svelte-scoped test:integration",
   "test:ci": "nyxr build && nr typecheck && nr lint && nr test && nr test:integration"
 },
